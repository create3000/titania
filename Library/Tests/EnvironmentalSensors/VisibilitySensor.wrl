#VRML V2.0 utf8

META "title" "VisibilitySensor"

Background {
  skyColor 1 1 1
}

Viewpoint { }

Transform {
  children Shape {
    appearance Appearance {
      material Material {
        diffuseColor 1 0 0
        transparency 0.5
      }
    }
    geometry Box {
      size 1 1 1
      solid FALSE
    }
  }
}

DEF Sensor VisibilitySensor { }

DEF Debug Script {
  eventIn SFBool set_active

  url "vrmlscript:
function set_active (value, time)
{
	print (value);
}
	"
}

ROUTE Sensor.isActive TO Debug.set_active
