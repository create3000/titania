#VRML V2.0 utf8

META "title" "LOD"

Background {
  skyColor 0.2 0.2 0.2
}

Transform {
  translation 12 -32 4
  rotation 0.250816 0.370772 0.894214 3.86726
  scale 12 45 9
  children [
    Viewpoint {
      description "Home"
    }
    DEF LOD LOD {
      range [ 20, 30, 40 ]
      level [
        Transform {
          children Shape {
            appearance Appearance {
              material Material {
                diffuseColor 1 0 0
              }
            }
            geometry Box { }
          }
        }
        Transform {
          children Shape {
            appearance Appearance {
              material Material {
                diffuseColor 0 1 0
              }
            }
            geometry Box { }
          }
        }
        Transform {
          children Shape {
            appearance Appearance {
              material Material {
                diffuseColor 0 0 1
              }
            }
            geometry Box { }
          }
        }
      ]
    }
    DEF Proxy ProximitySensor {
      size 1000 1000 1000
    }
  ]
}

DEF Debug Script {
  eventIn SFVec3f set_position
  eventIn SFInt32 set_level

  url "vrmlscript:
function set_position (value)
{
	print ('position: ' + value .length ());
}

function set_level (value)
{
	print ('level:    ' + value);
}
	"
}

ROUTE Proxy.position_changed TO Debug.set_position
ROUTE LOD.level_changed TO Debug.set_level