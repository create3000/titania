#X3D V3.0 utf8 Titania

META "title" "Walk viewer"

Transform {
  #scale 0.1 0.1 0.1
  children [
    NavigationInfo {
      type "WALK"
    }
    Viewpoint {
      description "Home"
      position 0 10 30
    }
    Viewpoint {
      description "Back"
      position 0 10 -30
      orientation 0 1 0 3.14158
    }
    DirectionalLight {
      direction 0 -1 0
    }
    DEF Floor Transform {
      scale 100 1 100
      children Shape {
        appearance Appearance {
          material Material { }
          texture ImageTexture {
            url "../images/image.png"
          }
        }
        geometry Box { }
      }
    }
    DEF Step1 Transform {
      scale 2 1.5 2
      children Shape {
        appearance Appearance {
          material Material { }
        }
        geometry Box { }
      }
    }
    DEF Step2 Collision {
      enabled FALSE
      children Transform {
        translation 0 0 -4
        scale 2 1.5 2
        children Shape {
          appearance Appearance {
            material Material {
              diffuseColor 1 0 0
            }
          }
          geometry Box { }
        }
      }
    }
    DEF Box Collision {
      children Transform {
        translation -10 0 -4
        scale 6 6 6
        children Shape {
          appearance Appearance {
            texture ImageTexture {
              url "../images/image.png"
            }
          }
          geometry Box {
            solid FALSE
          }
        }
      }
    }
    DEF Stairs Transform {
      translation 0 0 -4
      rotation -1 -0 -0 0.7
      scale 2 50 0.5
      children Shape {
        appearance Appearance {
          material Material { }
        }
        geometry Box { }
      }
    }
    DEF Roof Transform {
      translation 0 20 0
      scale 2 1 20
      children Shape {
        appearance Appearance {
          material Material { }
        }
        geometry Box { }
      }
    }
  ]
}

DEF Debug Script {
  inputOnly SFBool set_active
  inputOnly SFTime set_collideTime

  url "vrmlscript:
function set_active (value)
{
	print ('isActive:    ' + value);
}
function set_collideTime (value)
{
	print ('collideTime: ' + value);
}
"
}

ROUTE Box.isActive TO Debug.set_active
ROUTE Box.collideTime TO Debug.set_collideTime
