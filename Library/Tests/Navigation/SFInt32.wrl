#VRML V2.0 utf8

PROTO SFInt32 [
  exposedField SFInt32 keyValue 0
  exposedField SFTime  startTime 0
  eventOut     SFInt32 value_changed
]
{
  PROTO Fields [
    exposedField SFInt32 keyValue 0
    exposedField SFTime  startTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    startTime IS startTime
  }

  DEF SFInt32 Script {
    eventIn  SFTime  set_startTime
    eventOut SFInt32 value_changed IS value_changed
    field    SFNode  fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .startTime)
		set_startTime (0, 0);
}

function set_startTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.startTime_changed TO SFInt32.set_startTime
}


