  url "vrmlscript:
function set_trackPoint1 (value)
{
	print ('Touch 1 trackPoint: ' + value);
}

function set_isOver1 (value)
{
	print ('Touch 1 isOver: ' + value);
}

function set_isActive1 (value)
{
	print ('Touch 1 isActive: ' + value);
}
