"vrmlscript:",
"function set_dimension ()",
"{",
"}",
"",
"function set_spacing ()",
"{",
"}",
"",
"function initialize ()",
"{",
"\tfields .transparency = fields .transparency;",
"",
"\t// Plane",
"",
"\tplane .scale = new SFVec3f (fields .xDimension * fields .xSpacing, fields .zDimension * fields .zSpacing, 1);",
"\t",
"\t// Axis",
"",
"\taxis .coord .point = new MFVec3f (new SFVec3f (-plane .scale .x / 2 - fields .xSpacing / 4, 0, 0),",
"\t                                  new SFVec3f ( plane .scale .x / 2 + fields .xSpacing / 4, 0, 0),",
"\t                                  new SFVec3f (0, 0, -plane .scale .y / 2 - fields .zSpacing / 4),",
"\t                                  new SFVec3f (0, 0,  plane .scale .y / 2 + fields .zSpacing / 4));",
"  ",
"\taxisMaterial .transparency = Math .pow (fields .transparency, 3 / 4);",
"",
"\t// Grid",
"",
"\tvar coordIndex     = new MFInt32 ();",
"\tcoordIndex .length = 3 * (fields .xDimension + fields .zDimension);",
"\t",
"\tvar point     = grid .coord .point;",
"\tpoint .length = 2 * (fields .xDimension + fields .zDimension);",
"",
"\tvar i = 0;",
"\t",
"\tfor (; i < fields .xDimension + 1; ++ i)",
"\t{",
"\t\tpoint [2 * i]          = new SFVec3f (-plane .scale .x / 2 + fields .xSpacing * i, 0, -plane .scale .y / 2);",
"\t\tpoint [2 * i + 1]      = new SFVec3f (-plane .scale .x / 2 + fields .xSpacing * i, 0,  plane .scale .y / 2);",
"\t\tcoordIndex [3 * i]     = 2 * i;",
"\t\tcoordIndex [3 * i + 1] = 2 * i + 1;",
"\t\tcoordIndex [3 * i + 2] = -1;",
"\t}",
"\t",
"\tfor (; i < fields .zDimension + 1 + fields .xDimension + 1; ++ i)",
"\t{",
"\t\tpoint [2 * i]          = new SFVec3f (-plane .scale .x / 2, 0, -plane .scale .y / 2 + fields .zSpacing * (i - fields .xDimension - 1));",
