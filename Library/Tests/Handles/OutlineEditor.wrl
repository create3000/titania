#VRML V2.0 utf8

META "title" "OutlineEditor"

EXTERNPROTO Rotor [
  exposedField SFBool     enabled
  exposedField SFTime     cycleInterval
  exposedField SFBool     loop
  exposedField SFTime     startTime
  exposedField SFTime     stopTime
  eventOut     SFRotation value_changed
]
"Rotor.wrl"

EXTERNPROTO Grid [
  exposedField SFInt32 xDimension
  exposedField SFFloat xSpacing
  exposedField SFInt32 zDimension
  exposedField SFFloat zSpacing
  exposedField SFColor color
  exposedField SFFloat transparency
  field        SFBool  solid
]
"Grid.wrl"

PROTO Children1 [
  exposedField MFNode children1 [ ]
]
{
  PROTO Children2 [
    exposedField MFNode children2 [ ]
  ]
  {
    Transform {
      children IS children2
    }
  }

  Children2 {
    children2 IS children1
  }
}

NavigationInfo {
  type [ "NONE", "ANY" ]
}

Viewpoint {
  position 2.37398 0.256043 5.7671
  centerOfRotation 2.37398 0.256043 0
}

DEF Text Transform {
  children Shape {
    appearance Appearance {
      material Material { }
    }
    geometry DEF Text Text {
      string "OutlineEditor"
      fontStyle FontStyle { }
    }
  }
}

Transform {
  children [
    DEF _0 ProximitySensor { }
    VisibilitySensor { }
  ]
}

Group {
  children USE _0
}

DEF Rotor Rotor {
  loop TRUE
}

DEF Box Transform {
  translation 1.40843 1.10727 0.193803
  rotation 0 0 1 0.378658
  scale 0.2 0.2 0.2
  children Transform {
    children Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0.8 0 0
        }
      }
      geometry Box { }
    }
  }
}

DEF Sphere Transform {
  translation 2.31518 0.867383 0
  scale 0.2 0.2 0.2
  children Transform {
    children Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0 0.8 0
        }
      }
      geometry Sphere { }
    }
  }
}

DEF Cone Transform {
  translation 3.05759 1.14098 0
  scale 0.2 0.2 0.2
  children Transform {
    children Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0 0 0.8
        }
      }
      geometry Cone { }
    }
  }
}

Children1 {
  children1 [
    Transform { }
    Transform { }
  ]
}

Grid {
  xDimension 100
  zDimension 100
}

ROUTE Rotor.value_changed TO Box.set_rotation
