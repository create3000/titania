#VRML V2.0 utf8

DEF Timer TimeSensor {
  loop TRUE
}

DEF Interpolator PositionInterpolator {
  key [ 0, 1 ]
  keyValue [ 1 2 3, 1 2 3 ]
}

DEF From Script {
  eventIn  SFVec3f set_translation
  eventOut SFVec3f translation_changed

  url "vrmlscript:
function initialize ()
{
	print ('_schatten');
}                  

function set_translation (value, time)
{
	translation_changed = new SFVec3f (value.x, 0, value.z);
}
"
}

DEF Print Script {
  eventIn  SFVec3f set_translation

  url "vrmlscript:
function set_translation (value, time)
{
	print (value);
}
"
}

ROUTE Timer.fraction_changed TO Interpolator.set_fraction
ROUTE Interpolator.value_changed TO From.set_translation
ROUTE From .translation_changed TO Print.set_translation