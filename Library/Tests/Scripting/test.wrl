META "title" "Scripting test"

DEF Timer TimeSensor {
  loop TRUE
}

DEF Debug1 Script {
  eventIn SFTime set_time
  field   SFNode self USE Debug1

  url "vrmlscript:
function initialize ()
{
	print ('initialize1');
	
	var a = [];
	
	for (var i = 0; i < 400000; ++ i)
		a .push (new SFVec3f ());
		
	print (a .length);
	
	a .length = 0;
	
	print (a .length);
}

function set_time (value)
{
}

function shutdown ()
{
	print ('shutdown1');
}
"
}

ROUTE Timer.cycleTime TO Debug1.set_time
