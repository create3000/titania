var n = 0;

function fibonacci_recursive (value)
{
	n = n + 1;

	if (value === 0)
		return 0;

	if (value === 1)
		return 1;

	return fibonacci_recursive (value - 1) + fibonacci_recursive (value - 2);
}

function fibonacci_iterative (value)
{
	print ('fibonacci_iterative: ', value);

	if (value === 0)
		return 0;

	if (value === 1)
		return 1;

	var result;
	var h1 = 0;
	var h2 = 1;

	for (var i = 1; i < value; i = i + 1)
	{
		result = h1 + h2;
		h1     = h2;
		h2     = result;
	}

	return result;
}

