#VRML V2.0 utf8

PROTO MFVec2f [
  exposedField MFVec2f keyValue [ ]
  exposedField SFTime  startTime 0
  eventOut     MFVec2f value_changed
]
{
  PROTO Fields [
    exposedField MFVec2f keyValue [ ]
    exposedField SFTime  startTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    startTime IS startTime
  }

  DEF MFVec2f Script {
    eventIn  SFTime  set_startTime
    eventOut MFVec2f value_changed IS value_changed
    field    SFNode  fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .startTime)
		set_startTime (0, 0);
}

function set_startTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.startTime_changed TO MFVec2f.set_startTime
}


