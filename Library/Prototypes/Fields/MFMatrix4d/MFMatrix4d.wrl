#VRML V2.0 utf8

PROTO MFMatrix4d [
  exposedField MFMatrix4d keyValue [ ]
  exposedField SFTime     triggerTime 0
  eventOut     MFMatrix4d value_changed
]
{
  PROTO Fields [
    exposedField MFMatrix4d keyValue [ ]
    exposedField SFTime     triggerTime 0
  ]
  {
    Group { }
  }

  DEF MFMatrix4d Script {
    eventIn  SFTime     set_triggerTime
    eventOut MFMatrix4d value_changed IS value_changed
    field    SFNode     fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO MFMatrix4d.set_triggerTime
}

