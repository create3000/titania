#VRML V2.0 utf8

PROTO MFMatrix4d [
  exposedField MFMatrix4d keyValue [ ]
  exposedField SFTime     startTime 0
  eventOut     MFMatrix4d value_changed
]
{
  PROTO Fields [
    exposedField MFMatrix4d keyValue [ ]
    exposedField SFTime     startTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    startTime IS startTime
  }

  DEF MFMatrix4d Script {
    eventIn  SFTime     set_startTime
    eventOut MFMatrix4d value_changed IS value_changed
    field    SFNode     fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .startTime)
		set_startTime (0, 0);
}

function set_startTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.startTime_changed TO MFMatrix4d.set_startTime
}


