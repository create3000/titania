#VRML V2.0 utf8

PROTO SFFloat [
  exposedField SFFloat keyValue 0
  exposedField SFTime  triggerTime 0
  eventOut     SFFloat value_changed
]
{
  PROTO Fields [
    exposedField SFFloat keyValue 0
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF SFFloat Script {
    eventIn  SFTime  set_triggerTime
    eventOut SFFloat value_changed IS value_changed
    field    SFNode  fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFFloat.set_triggerTime
}

