#VRML V2.0 utf8

PROTO SFRotation [
  exposedField SFRotation keyValue 0 0 1 0
  exposedField SFTime     startTime 0
  eventOut     SFRotation value_changed
]
{
  PROTO Fields [
    exposedField SFRotation keyValue 0 0 1 0
    exposedField SFTime     startTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    startTime IS startTime
  }

  DEF SFRotation Script {
    eventIn  SFTime     set_startTime
    eventOut SFRotation value_changed IS value_changed
    field    SFNode     fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .startTime)
		set_startTime (0, 0);
}

function set_startTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.startTime_changed TO SFRotation.set_startTime
}


