#VRML V2.0 utf8

PROTO SFVec4f [
  exposedField SFVec4f keyValue 0 0 0 0
  exposedField SFTime  triggerTime 0
  eventOut     SFVec4f value_changed
]
{
  PROTO Fields [
    exposedField SFVec4f keyValue 0 0 0 0
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    triggerTime IS triggerTime
  }

  DEF SFVec4f Script {
    eventIn  SFTime  set_triggerTime
    eventOut SFVec4f value_changed IS value_changed
    field    SFNode  fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFVec4f.set_triggerTime
}


