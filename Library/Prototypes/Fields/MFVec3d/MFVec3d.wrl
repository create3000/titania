#VRML V2.0 utf8

PROTO MFVec3d [
  exposedField MFVec3d keyValue [ ]
  exposedField SFTime  triggerTime 0
  eventOut     MFVec3d value_changed
]
{
  PROTO Fields [
    exposedField MFVec3d keyValue [ ]
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF MFVec3d Script {
    eventIn  SFTime  set_triggerTime
    eventOut MFVec3d value_changed IS value_changed
    field    SFNode  fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO MFVec3d.set_triggerTime
}

