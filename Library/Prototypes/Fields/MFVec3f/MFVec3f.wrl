#VRML V2.0 utf8

PROTO MFVec3f [
  exposedField MFVec3f keyValue [ ]
  exposedField SFTime  triggerTime 0
  eventOut     MFVec3f value_changed
]
{
  PROTO Fields [
    exposedField MFVec3f keyValue [ ]
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    triggerTime IS triggerTime
  }

  DEF MFVec3f Script {
    eventIn  SFTime  set_triggerTime
    eventOut MFVec3f value_changed IS value_changed
    field    SFNode  fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO MFVec3f.set_triggerTime
}


