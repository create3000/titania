#VRML V2.0 utf8

PROTO SFMatrix3d [
  exposedField SFMatrix3d keyValue 1 0 0 0 1 0 0 0 1
  exposedField SFTime     triggerTime 0
  eventOut     SFMatrix3d value_changed
]
{
  PROTO Fields [
    exposedField SFMatrix3d keyValue 1 0 0 0 1 0 0 0 1
    exposedField SFTime     triggerTime 0
  ]
  {
    Group { }
  }

  DEF SFMatrix3d Script {
    eventIn  SFTime     set_triggerTime
    eventOut SFMatrix3d value_changed IS value_changed
    field    SFNode     fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFMatrix3d.set_triggerTime
}

