#VRML V2.0 utf8

PROTO SFString [
  exposedField SFString keyValue ""
  exposedField SFTime   triggerTime 0
  eventOut     SFString value_changed
]
{
  PROTO Fields [
    exposedField SFString keyValue ""
    exposedField SFTime   triggerTime 0
  ]
  {
    Group { }
  }

  DEF SFString Script {
    eventIn  SFTime   set_triggerTime
    eventOut SFString value_changed IS value_changed
    field    SFNode   fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFString.set_triggerTime
}

