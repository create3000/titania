#VRML V2.0 utf8

PROTO SFDouble [
  exposedField SFDouble keyValue 0
  exposedField SFTime   triggerTime 0
  eventOut     SFDouble value_changed
]
{
  PROTO Fields [
    exposedField SFDouble keyValue 0
    exposedField SFTime   triggerTime 0
  ]
  {
    Group { }
  }

  DEF SFDouble Script {
    eventIn  SFTime   set_triggerTime
    eventOut SFDouble value_changed IS value_changed
    field    SFNode   fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFDouble.set_triggerTime
}

