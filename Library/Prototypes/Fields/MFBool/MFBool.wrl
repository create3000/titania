#VRML V2.0 utf8

PROTO MFBool [
  exposedField MFBool keyValue [ ]
  exposedField SFTime triggerTime 0
  eventOut     MFBool value_changed
]
{
  PROTO Fields [
    exposedField MFBool keyValue [ ]
    exposedField SFTime triggerTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    triggerTime IS triggerTime
  }

  DEF MFBool Script {
    eventIn  SFTime set_triggerTime
    eventOut MFBool value_changed IS value_changed
    field    SFNode fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO MFBool.set_triggerTime
}


