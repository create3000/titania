#VRML V2.0 utf8

PROTO SFVec2d [
  exposedField SFVec2d keyValue 0 0
  exposedField SFTime  triggerTime 0
  eventOut     SFVec2d value_changed
]
{
  PROTO Fields [
    exposedField SFVec2d keyValue 0 0
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF Fields Fields {
    keyValue IS keyValue
    triggerTime IS triggerTime
  }

  DEF SFVec2d Script {
    eventIn  SFTime  set_triggerTime
    eventOut SFVec2d value_changed IS value_changed
    field    SFNode  fields USE Fields

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO SFVec2d.set_triggerTime
}


