#VRML V2.0 utf8

PROTO MFVec4f [
  exposedField MFVec4f keyValue [ ]
  exposedField SFTime  triggerTime 0
  eventOut     MFVec4f value_changed
]
{
  PROTO Fields [
    exposedField MFVec4f keyValue [ ]
    exposedField SFTime  triggerTime 0
  ]
  {
    Group { }
  }

  DEF MFVec4f Script {
    eventIn  SFTime  set_triggerTime
    eventOut MFVec4f value_changed IS value_changed
    field    SFNode  fields DEF Fields Fields {
      keyValue IS keyValue
      triggerTime IS triggerTime
    }

    url "vrmlscript:
function initialize ()
{
	if (fields .triggerTime)
		set_triggerTime (0, 0);
}

function set_triggerTime (value, time)
{
	value_changed = fields .keyValue;
}
"
    directOutput TRUE
  }

  ROUTE Fields.triggerTime_changed TO MFVec4f.set_triggerTime
}

