#VRML V2.0 utf8

META "title" "QuadSphere"

EXTERNPROTO QuadSphere [
  exposedField SFInt32 uDimension
  exposedField SFInt32 vDimension
  exposedField SFFloat radius
  field        SFBool  solid
  field        SFFloat creaseAngle
]
"QuadSphere.wrl"

Viewpoint {
  position 0 0 4
}

Transform {
  children [
    Shape {
      appearance Appearance {
        material Material { }
        texture ImageTexture {
          url "earth.png"
        }
      }
      geometry QuadSphere {
        uDimension 8
        vDimension 5
        creaseAngle 0
      }
    }
    Shape {
      geometry IndexedLineSet {
        coordIndex [ 0, 1, -1 ]
        coord Coordinate {
          point [ 0 -2 0, 0 2 0 ]
        }
      }
    }
  ]
}
