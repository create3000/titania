#VRML V2.0 utf8

META "title" "Button"

EXTERNPROTO Button [
  exposedField SFBool enabled
  eventOut     SFBool isOver
  eventOut     SFBool isActive
  eventOut     SFTime touchTime
  eventIn      MFNode addChildren
  eventIn      MFNode removeChildren
  exposedField MFNode children
]
"Button.wrl"

Button {
  children [
    DEF Default Shape {
      appearance Appearance {
        material Material { }
      }
      geometry Box { }
    }
    DEF Over Shape {
      appearance Appearance {
        material Material {
          diffuseColor 1 1 1
        }
      }
      geometry Box { }
    }
    DEF Active Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0.4 0.4 0.4
        }
      }
      geometry Box { }
    }
    USE Default
  ]
}

