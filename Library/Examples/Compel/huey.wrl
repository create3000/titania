	bodyHeight = bodyLoc[1];
	armRot = armBaseRot + armRotationMultiplier * bodyHeight;
	rightArmRotation = new SFRotation(0,0,1,armRot);
	leftArmRotation = new SFRotation(0,0,-1,armRot);
