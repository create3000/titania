      field      SFBool      simulationOn	FALSE
      field      SFTime      prevTime	-1
      field      SFTime      timeStepSize	0.05
      field      SFFloat      mass	1
      field      SFFloat      springConstant	100
      field      SFFloat      dampingConstant	1
