#VRML V2.0 utf8

PROTO TimeField [
  exposedField  SFTime   keyValue		0
  exposedField  SFTime    startTime		0
  eventOut      SFTime   value_changed
]
{
  PROTO Data [
    exposedField  SFTime   keyValue		0
    exposedField  SFTime    startTime		0
  ]
  {
    Group { }
  }

  Group {
  	children DEF _Time Script { 
    eventIn   SFTime    set_startTime
    eventOut  SFTime   value_changed IS value_changed
    field     SFNode    data DEF Data Data { 
      keyValue IS keyValue
      startTime IS startTime
    }
    url "vrmlscript:
function set_startTime(value, time) { value_changed = data.keyValue; }
function initialize() { if (data.startTime) set_startTime(0, 0); }
"
    directOutput TRUE
  }
  }

  ROUTE Data.startTime_changed TO _Time.set_startTime
}

