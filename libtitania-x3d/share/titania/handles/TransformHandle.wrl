function set_active (value)
{
	if (value)
		rotation = transform .rotation;
}

function set_xRotation (value)
{
	transform .rotation = value .multiply (rotation);
}

function set_yRotation (value)
{
	transform .rotation = value .multiply (rotation);
}

function set_zRotation (value)
{
	transform .rotation = value .multiply (rotation);
}
"
    directOutput TRUE
  }

