  DEF MoveScript Script {
    inputOnly      SFBool  set_active
    inputOnly      SFVec3f set_frontTranslation
    inputOnly      SFVec3f set_rightTranslation
    inputOnly      SFVec3f set_backTranslation
    inputOnly      SFVec3f set_leftTranslation
