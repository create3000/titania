function initialize ()
{
	set_whichChoice (0);
}

function set_enabled (value, time)
{
	move = value;
}

function set_active (value, time)
{
	if (! value)
		touchTime = time;
}

function set_time (value, time)
{
	set_whichChoice ((whichChoice_changed + 1) % 3);
}

function set_whichChoice (value)
{
	move   = false;
	rotate = false;
	scale  = false;
	
	switch (value)
	{
		case 0:
			move = true;
			break;
		case 1:
			move   = true;
			rotate = true;
			break;
		case 2:
			scale = true;
			break;
	}

	whichChoice_changed = value;
}
"
