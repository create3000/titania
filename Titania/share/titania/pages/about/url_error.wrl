#X3D V3.0 utf8 Titania

META "icon" "about:icon"
META "title" "about:url_error"

EXTERNPROTO Red [
  inputOutput SFFloat transparency
]
"../../library/Colors.wrl"

NavigationInfo {
  type "NONE"
}

Inline {
  url "../../library/background.wrl"
}

Viewpoint { }

Transform {
  translation -5 0 0
  children Shape {
    appearance Appearance {
      material Red { }
    }
    geometry DEF Message Text {
      string ""
      fontStyle FontStyle {
        family "Ubuntu"
        size 0.3
      }
    }
  }
}

Script {
  initializeOnly SFNode message USE Message

  url "vrmlscript:

function initialize ()
{
	print ('### worldURL: ' + Browser .getWorldURL ());
	
	message .string = new MFString (Browser .getWorldURL ());
}
	"
}
