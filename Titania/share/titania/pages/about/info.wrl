#X3D V3.0 utf8 Titania

META "icon" "../../ui/icons/icon-bw.svg"
META "title" "about:info"

EXTERNPROTO Rotor [
  inputOutput SFTime     cycleInterval
  inputOutput SFBool     loop
  inputOutput SFTime     startTime
