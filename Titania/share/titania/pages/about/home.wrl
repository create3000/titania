#X3D V3.0 utf8 Titania

META "title" "about:home"
META "icon"  "about:icon"

EXTERNPROTO Rotor [
  inputOutput SFTime     cycleInterval
  inputOutput SFBool     loop
  outputOnly  SFRotation value_changed
]
"../Library/Rotor.wrl"

DEF NavigationInfo NavigationInfo {
  headlight FALSE
}

DEF Home Viewpoint {
  position 0 0 34
  description "Home"
}

DEF Far Viewpoint {
  position 0 0 68
  description "Far"
}

DEF White Background {
  skyColor 1 1 1
  transparency 1
}

DEF Blue Background {
  skyColor 0.25098 0.270588 0.329412
  transparency 1
}

DEF DarkBlue Background {
  skyColor 0.231373 0.243137 0.301961
  transparency 1
}

DEF Fastener Rotor {
  cycleInterval 60
  loop TRUE
}

LayerSet {
  order [ 1, 2 ]
  layers [
    DEF _0 Layer {
      children [
        USE NavigationInfo
        USE Home
        USE Far
        DEF Lesser Transform {
          children DEF loops Inline {
            url "home/red.orange.yellow.wrl"
          }
        }
      ]
    }
    DEF _1 Layer {
      children [
        USE NavigationInfo
        USE Home
        USE Far
        DEF Greater Transform {
          scale 0.61803 0.61803 1
          children USE loops
        }
      ]
    }
  ]
}


#ROUTE Fastener.value_changed TO Lesser.rotation
