#X3D V3.0 utf8 Titania

META "icon" "about:icon"
META "title" "about:home"

EXTERNPROTO Rotor [
  inputOutput SFBool     enabled
  inputOutput SFTime     cycleInterval
  inputOutput SFBool     loop
  inputOutput SFTime     startTime
  inputOutput SFTime     stopTime
  outputOnly  SFRotation value_changed
]
"../../library/Rotor.wrl"

DEF NavigationInfo NavigationInfo { }

DEF Home Viewpoint {
  description "Home"
  position 0 0 34
}

DEF Far Viewpoint {
  description "Far"
  position 0 0 68
}

DEF White Background {
  skyColor 1 1 1
}

DEF Blue Background {
  skyColor 0.25098 0.270588 0.329412
  transparency 1
}

DEF DarkBlue Background {
  skyColor 0.231373 0.243137 0.301961
  transparency 1
}

DEF Fastener Rotor {
  cycleInterval 61
  loop TRUE
}

LayerSet {
  order [ 0, 1, 2 ]
  layers [
    DEF _0 Layer {
      children [
        USE NavigationInfo
        USE Home
        USE Far
        DEF Greater Transform {
          children DEF loops Inline {
            url "home/red.orange.yellow.wrl"
          }
        }
      ]
    }
    DEF _1 Layer {
      children [
        USE NavigationInfo
        USE Home
        USE Far
        DEF Lesser Transform {
          scale 0.236068 0.236068 1
          children USE loops
        }
      ]
    }
  ]
}

ROUTE Fastener.value_changed TO Lesser.set_rotation
