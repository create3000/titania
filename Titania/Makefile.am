install-exec-hook:
	mkdir -m 755 -p $(DESTDIR)$(datadir)
	cp -r share/* $(DESTDIR)$(datadir)
	find $(DESTDIR)$(datadir)/titania/ -type d -exec chmod 755 {} \;
	find $(DESTDIR)$(datadir)/titania/ -type f -exec chmod 644 {} \;

run:
	$(builddir)/run.pl 2>&1
